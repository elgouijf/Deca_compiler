; test de la taille de la pile
	TSTO #4	; Taille maximale de la pile
	BOV stack_overflow_error
	ADDSP #3
; start main program
; Construction de la table des m√©thodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main program
; Beginning of main declarations:
	LOAD #null, R2
	STORE R2, 3(GB)
; Beginning of main instructions:
	NEW #1, R2
	BOV tas_plein
	LEA 1(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.Object
	POP R2
	STORE R2, 3(GB)
	HALT
; end main program
init.Object:
	RTS
code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
stack_overflow_error:
	WSTR "Error : Stack Overflow"
	WNL
	ERROR
tas_plein:
	WSTR "Error : Heap OverFlow"
	WNL
	ERROR
; end main program

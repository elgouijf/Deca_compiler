; Initialisation des champs de la classe Chien
init.Chien:
	TSTO #5
	PUSH R2
	PUSH R3
	BOV stack_overflow_error
	LOAD -2(LB), R1
	LOAD #0, R0
	STORE R0, 2(R1)
	PUSH R1
	BSR init.Animal
	SUBSP #1
	LOAD -2(LB), R1
	LOAD -2(LB), R3
	LOAD #4, R1
	STORE R1, 2(R3)
	RTS
	POP R3
	POP R2

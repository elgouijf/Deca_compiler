; test de la taille de la pile
	TSTO #13	; Taille maximale de la pile
	ADDSP #12
	BOV stack_overflow_error
; start main program
; Construction de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Construction de la table des méthodes de A
	LEA 1(GB), R0
	STORE R0, 3(GB)
	LOAD code.Object.equals, R0
	STORE R0, 4(GB)
	LOAD code.A.setI, R0
	STORE R0, 5(GB)
	LOAD code.A.asmA, R0
	STORE R0, 6(GB)
; Construction de la table des méthodes de B
	LEA 1(GB), R0
	STORE R0, 7(GB)
	LOAD code.Object.equals, R0
	STORE R0, 8(GB)
	LOAD code.B.asmB, R0
	STORE R0, 9(GB)
	LOAD code.B.printvalI, R0
	STORE R0, 10(GB)
; Main program
; Beginning of main declarations:
; new ligne 4
	NEW #2, R2
	BOV tas_plein
	LEA 3(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.A
	POP R2
	STORE R2, 11(GB)
; new ligne 14
	NEW #2, R2
	BOV tas_plein
	LEA 7(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.B
	POP R2
	STORE R2, 12(GB)
; Beginning of main instructions:
	ADDSP #1
	LOAD 11(GB), R3
	STORE R3, 0(SP)
	LOAD 0(SP), R3
	CMP #null, R3
	BEQ dereferencement_null
	LOAD 0(R3), R3
	BSR 3(R3)
	SUBSP #1
	ADDSP #1
	LOAD 12(GB), R3
	STORE R3, 0(SP)
	LOAD 0(SP), R3
	CMP #null, R3
	BEQ dereferencement_null
	LOAD 0(R3), R3
	BSR 2(R3)
	SUBSP #1
	ADDSP #1
	LOAD 12(GB), R3
	STORE R3, 0(SP)
	LOAD 0(SP), R3
	CMP #null, R3
	BEQ dereferencement_null
	LOAD 0(R3), R3
	BSR 3(R3)
	SUBSP #1
	HALT
; end main program
code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
; Initialisation des champs de la classe A
init.A:
	TSTO #1
	PUSH R2
; Entree dans la boucle des fields
	LOAD -2(LB), R2
	LOAD #8, R1
	STORE R1, 1(R2)
	POP R2
	RTS
code.A.setI:
	TSTO #2
	PUSH R2
	PUSH R3
	LOAD -3(LB), R2
	LOAD -2(LB), R3
	CMP #null, R3
	BEQ dereferencement_null
	PUSH R3
	LOAD -3(LB), R2
	POP R3
	STORE R2, 1(R3)
fin.A.setI:
	POP R3
	POP R2
	RTS
code.A.asmA:
	WSTR "JE SUIS UN A "
fin.A.asmA:
	RTS
; Initialisation des champs de la classe B
init.B:
	TSTO #2
	PUSH R2
	PUSH R3
; Entree dans la boucle des fields
	LOAD -2(LB), R2
	LOAD #17, R1
	STORE R1, 1(R2)
	POP R3
	POP R2
	RTS
code.B.asmB:
	WSTR "JE SUIS UN B "
fin.B.asmB:
	RTS
code.B.printvalI:
	LOAD #17,R15
    LOAD R15,R1
    WINT
fin.B.printvalI:
	RTS
stack_overflow_error:
	WSTR "Error : Stack Overflow"
	WNL
	ERROR
tas_plein:
	WSTR "Error : Heap OverFlow"
	WNL
	ERROR
dereferencement_null:
	WSTR "Error : Dereferencing null"
	WNL
	ERROR
; end main program

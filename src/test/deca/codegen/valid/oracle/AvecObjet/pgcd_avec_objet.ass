code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
; Initialisation des champs de la classe Calc
init.Calc:
	RTS
code.Calc.pgcd:
	TSTO #1
	PUSH R2
	LOAD -4(LB), R2
	CMP #0, R2
	BNE E_Sinon.0
	LOAD -3(LB), R0
	BRA fin.Calc.pgcd
	BRA E_Fin.0
E_Sinon.0:
	ADDSP #3
	LOAD -2(LB), R2
	STORE R2, 0(SP)
	LOAD -4(LB), R2
	STORE R2, -1(SP)
	LOAD -3(LB), R2

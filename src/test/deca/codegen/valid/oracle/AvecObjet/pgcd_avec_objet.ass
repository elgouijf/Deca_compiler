	CMP #null, R2
	BEQ dereferencement_null
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #3
	BRA fin.Calc.pgcd
E_Fin.0:
fin.Calc.pgcd:
	POP R2
	RTS
stack_overflow_error:

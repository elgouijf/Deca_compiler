; test de la taille de la pile
	TSTO #9	; Taille maximale de la pile
	ADDSP #7
	BOV stack_overflow_error
; start main program
; Construction de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Construction de la table des méthodes de Fib
	LEA 1(GB), R0
	STORE R0, 3(GB)
	LOAD code.Object.equals, R0
	STORE R0, 4(GB)
	LOAD code.Fib.compute, R0
	STORE R0, 5(GB)
; Main program
; Beginning of main declarations:
; new ligne 11
	NEW #1, R2
	BOV tas_plein
	LEA 3(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.Fib
	POP R2
	STORE R2, 6(GB)
	LOAD #0, R1
	STORE R1, 7(GB)
; Beginning of main instructions:
	BRA E_Cond.0
E_Debut.0:
	ADDSP #2
	LOAD 6(GB), R2
	STORE R2, 0(SP)
	LOAD 7(GB), R2
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement_null
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #2
	LOAD R0, R1
	WINT
	WSTR " "
	LOAD 7(GB), R2
	ADD #1, R2
	BOV overflow_error
	STORE R2, 7(GB)
E_Cond.0:
	LOAD 7(GB), R2
	CMP #10, R2
	BLT E_Debut.0
	WNL
	HALT
; end main program
code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
; Initialisation des champs de la classe Fib
init.Fib:
	RTS
code.Fib.compute:
	TSTO #1
	PUSH R2
	LOAD -3(LB), R2
	CMP #1, R2
	BGT E_Sinon.1
	LOAD -3(LB), R0
	BRA fin.Fib.compute
	BRA E_Fin.1
E_Sinon.1:
	ADDSP #2
	LOAD -2(LB), R2
	STORE R2, 0(SP)
	LOAD -3(LB), R2
	SUB #1, R2
	BOV overflow_error
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement_null
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #2
	PUSH R0
	ADDSP #2
	LOAD -2(LB), R2
	STORE R2, 0(SP)
	LOAD -3(LB), R2
	SUB #2, R2
	BOV overflow_error
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement_null
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #2
	LOAD R0, R1
	POP R0
	ADD R1, R0
	BOV overflow_error
	BRA fin.Fib.compute
E_Fin.1:
fin.Fib.compute:
	POP R2
	RTS
stack_overflow_error:
	WSTR "Error : Stack Overflow"
	WNL
	ERROR
overflow_error:
	WSTR "Error : Overflow during arithmetic operation"
	WNL
	ERROR
io_error:
	WSTR "Error : Input / Output Error "
	WNL
	ERROR
tas_plein:
	WSTR "Error : Heap OverFlow"
	WNL
	ERROR
dereferencement_null:
	WSTR "Error : Dereferencing null"
	WNL
	ERROR
; end main program

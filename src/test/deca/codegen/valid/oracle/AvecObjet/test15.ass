; test de la taille de la pile
	TSTO #9	; Taille maximale de la pile
	ADDSP #7
	BOV stack_overflow_error
; start main program
; Construction de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Construction de la table des méthodes de MathUtil
	LEA 1(GB), R0
	STORE R0, 3(GB)
	LOAD code.Object.equals, R0
	STORE R0, 4(GB)
	LOAD code.MathUtil.double, R0
	STORE R0, 5(GB)
	LOAD code.MathUtil.quadruple, R0
	STORE R0, 6(GB)
; Main program
; Beginning of main declarations:
; new ligne 9
	NEW #1, R2
	BOV tas_plein
	LEA 3(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.MathUtil
	POP R2
	STORE R2, 7(GB)
; Beginning of main instructions:
	WSTR "quad(3) = "
	ADDSP #2
	LOAD 7(GB), R2
	STORE R2, 0(SP)
	LOAD #3, R2
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement_null
	LOAD 0(R2), R2

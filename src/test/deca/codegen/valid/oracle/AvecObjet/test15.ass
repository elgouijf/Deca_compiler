code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
; Initialisation des champs de la classe MathUtil
init.MathUtil:
	RTS
code.MathUtil.double:
	LOAD -3(LB), R0
	MUL #2, R0
	BOV overflow_error
	BRA fin.MathUtil.double
	BRA fin.MathUtil.double
etiq_fin0:
	WSTR "Erreur : sortie de la methode fin.MathUtil.doublesans return"
	WNL
	ERROR
fin.MathUtil.double:
	RTS
code.MathUtil.quadruple:
	TSTO #2
	PUSH R2
	PUSH R3
	ADDSP #2
	LOAD -2(LB), R2
	STORE R2, 0(SP)
	ADDSP #2
	LOAD -2(LB), R3
	STORE R3, 0(SP)
	LOAD -3(LB), R3
	STORE R3, -1(SP)
	LOAD 0(SP), R3
	CMP #null, R3
	BEQ dereferencement_null
	LOAD 0(R3), R3
	BSR 2(R3)
	SUBSP #2
	LOAD R0, R2
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement_null
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #2
	BRA fin.MathUtil.quadruple
	BRA fin.MathUtil.quadruple
etiq_fin1:
	WSTR "Erreur : sortie de la methode fin.MathUtil.quadruplesans return"
	WNL
	ERROR
fin.MathUtil.quadruple:
	POP R3
	POP R2
	RTS
stack_overflow_error:
	WSTR "Error : Stack Overflow"
	WNL
	ERROR
overflow_error:
	WSTR "Error : Overflow during arithmetic operation"
	WNL
	ERROR
io_error:
	WSTR "Error : Input / Output Error "

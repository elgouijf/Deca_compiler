; test de la taille de la pile
	TSTO #6	; Taille maximale de la pile
	ADDSP #6
	BOV stack_overflow_error
; start main program
; Construction de la table des m√©thodes de Object
	LOAD #null, R0
	LOAD code.Object.equals, R0
; Main program
; Beginning of main declarations:
	LOAD #0x1.cp0, R1
	STORE R1, 3(GB)
	LOAD #1, R1
	LOAD #0x1.0p-2, R1
	STORE R1, 5(GB)
	LOAD #2, R1
	STORE R1, 6(GB)
; Beginning of main instructions:
	LOAD #1, R1
	ADD 6(GB), R1
	BOV overflow_error
	WINT
	WNL
	LOAD 5(GB), R1
	ADD 3(GB), R1
	BOV overflow_error
	WFLOAT
	WNL
	HALT
; end main program
code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
stack_overflow_error:
	WSTR "Error : Stack Overflow"
	WNL
	ERROR
overflow_error:
	WSTR "Error : Overflow during arithmetic operation"
	WNL
	ERROR
io_error:
	WSTR "Error : Input / Output Error "
	WNL
	ERROR
; end main program

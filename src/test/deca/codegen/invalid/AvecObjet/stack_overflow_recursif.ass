	TSTO #8	; Taille maximale de la pile
	ADDSP #6
	BOV stack_overflow_error
; start main program
; Construction de la table des méthodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Construction de la table des méthodes de A
	LEA 1(GB), R0
	STORE R0, 3(GB)
	LOAD code.Object.equals, R0
	STORE R0, 4(GB)
	LOAD code.A.f, R0
	STORE R0, 5(GB)
; Main program
; Beginning of main declarations:
; new ligne 8
	NEW #1, R2
	BOV tas_plein
	LEA 3(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.A
	POP R2
	STORE R2, 6(GB)
; Beginning of main instructions:
	ADDSP #2
	LOAD 6(GB), R3
	STORE R3, 0(SP)
	LOAD #1, R3
	STORE R3, -1(SP)
	LOAD 0(SP), R3
	CMP #null, R3

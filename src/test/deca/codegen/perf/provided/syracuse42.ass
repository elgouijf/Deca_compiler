; test de la taille de la pile
	TSTO #4	; Taille maximale de la pile
	ADDSP #4
	BOV stack_overflow_error
; start main program
; Construction de la table des m√©thodes de Object
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main program
; Beginning of main declarations:
	LOAD #42, R1
	STORE R1, 3(GB)
	LOAD #0, R1
	STORE R1, 4(GB)
; Beginning of main instructions:
	BRA E_Cond.0
E_Debut.0:
	LOAD 3(GB), R2
	REM #2, R2
	BOV division_by_zero_error
	CMP #0, R2
	BNE E_Sinon.1
	LOAD 3(GB), R2
	QUO #2, R2
	BOV division_by_zero_error
	STORE R2, 3(GB)
	BRA E_Fin.1
E_Sinon.1:
	LOAD #2, R2
	MUL 3(GB), R2
	BOV overflow_error
	ADD 3(GB), R2
	BOV overflow_error
	ADD #1, R2
	BOV overflow_error
	STORE R2, 3(GB)
E_Fin.1:
	LOAD 4(GB), R2
	ADD #1, R2
	BOV overflow_error
	STORE R2, 4(GB)
E_Cond.0:
	LOAD 3(GB), R2
	CMP #1, R2
	BNE E_Debut.0
	LOAD 4(GB), R1
	WINT
	WNL
	HALT
; end main program
code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	BEQ equal
not_equal:
	LOAD #0, R0
	RTS
equal:
	LOAD #1, R0
	RTS
stack_overflow_error:
	WSTR "Error : Stack Overflow"
	WNL
	ERROR
overflow_error:
	WSTR "Error : Overflow during arithmetic operation"
	WNL
	ERROR
division_by_zero_error:
	WSTR "Error : Division by zero is forbidden"
	WNL
	ERROR
io_error:
	WSTR "Error : Input / Output Error "
	WNL
	ERROR
; end main program
